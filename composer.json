{
  "name": "neighborhoods/kojo-worker-decorator-component",
  "description": "A set of Kojo Worker Decorators that fulfill often repeated code.",
  "minimum-stability": "stable",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.neighborhoods.com"
    }
  ],
  "authors": [
    {
      "name": "Oleg Kulyk",
      "email": "olegkulik1985@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-pdo": "*",
    "neighborhoods/kojo": "^5",
    "neighborhoods/exception-component": "^1",
    "neighborhoods/throwable-diagnostic-component": "^3.0",
    "neighborhoods/container-builder": "^1.0"
  },
  "require-dev": {
    "ext-json": "*",
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.2",
    "phpstan/phpstan": "^0.12.62",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "phpstan/extension-installer": "^1.1",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Neighborhoods\\KojoWorkerDecoratorComponent\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neighborhoods\\KojoWorkerDecoratorComponent\\Tests\\": "tests"
    }
  }
}
